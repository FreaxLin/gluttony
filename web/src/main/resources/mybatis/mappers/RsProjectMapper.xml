<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.interc.crawler.mapper.RsProjectMapper">
  <resultMap id="BaseResultMap" type="top.interc.crawler.model.RsProject">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="ch_name" jdbcType="VARCHAR" property="chName" />
    <result column="project_name" jdbcType="VARCHAR" property="projectName" />
    <result column="description" jdbcType="VARCHAR" property="description" />
    <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    <result column="update_date" jdbcType="TIMESTAMP" property="updateDate" />
  </resultMap>

  <sql id="Base_Column_List">
    id, ch_name, project_name, description, create_date, update_date
  </sql>

  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from rs_project
    where id = #{id,jdbcType=INTEGER}
  </delete>

</mapper>